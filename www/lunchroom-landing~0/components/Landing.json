            "innerHTML": "\n    <!--<Navbar>-->\n<header class=\"navbar navbar-transparent navbar-transparent-dark\">\n  <div class=\"container\">\n    <a href=\"/\">\n      <img src=\"/img/logo_goodybag_white.png\" alt=\"Goodybag.com\" class=\"navbar-logo\">\n    </a>\n\n    <ul class=\"nav navbar-nav navbar-right\">\n      <li>\n        <a href=\"#\" data-toggle=\"modal\" data-target=\"#contact-us-modal\">Contact Us</a>\n      </li>\n    </ul>\n  </div>\n</header>\n<!--</Navbar>-->\n\n<section class=\"page-section hero-unit primary-section\" id=\"email-signup-cta\">\n  <div class=\"container\">\n    <h1 class=\"section-header\">Get your lunch delivered every day.</h1>\n    <p>Your choice of meal brought to you at work. Starts August 17th, 2015.</p>\n    <form action=\"#\" class=\"cta-form form\" component:id=\"subscribe\">\n      <div class=\"form-group\">\n        <div class=\"email-wrapper\">\n          <input type=\"email\" name=\"email\">\n        </div>\n        <div class=\"submit-wrapper\">\n          <button class=\"btn btn-primary\">Get notified</button>\n        </div>\n      </div>\n      <p class=\"form-explanation\">Enter your email to receive daily menus when we launch!</p>\n    </form>\n    <h2 class=\"outro-header\">Share the love! Send this page to your coworkers.</h2>\n  </div>\n</section>\n\n<section class=\"page-section\" id=\"how-it-works\">\n  <div class=\"container\">\n    <h1 class=\"section-title\">How it'll work</h1>\n    <div class=\"marketing-points\">\n      <div class=\"point\">\n        <img src=\"/img/icons_step1.png\" alt=\"Restaurant rotation\">\n        <p>One restaurant featured each day.</p>\n      </div>\n      <div class=\"point\">\n        <img src=\"/img/icons_step2.png\" alt=\"Order deadline\">\n        <p>Order by 10 AM.</p>\n      </div>\n      <div class=\"point\">\n        <img src=\"/img/icons_step3.png\" alt=\"Delivery notification\">\n        <p>We'll deliver your food at 12 and text you when it arrives!</p>\n      </div>\n    </div>\n  </div>\n</section>\n\n<section class=\"page-section section-dark\" id=\"new-menu-everyday\">\n  <div class=\"container\">\n    <h1 class=\"section-title\">A new menu every day</h1>\n    <img src=\"/img/browsers_3mockups.png\" class=\"section-screenshot\" alt=\"Menu screenshot\">\n  </div>\n</section>\n\n<section class=\"page-section\" id=\"restaurants\">\n  <div class=\"container\">\n    <h1 class=\"section-title\">A few restaurants you can expect</h1>\n    <ul class=\"logo-list\">\n      <li><img src=\"/img/logo_trudys.png\" alt=\"Trudy's\"></li>\n      <li><img src=\"/img/logo_micklethwait.png\" alt=\"Micklethwait\"></li>\n      <li><img src=\"/img/logo_torchys.png\" alt=\"Torchy's Tacos\"></li>\n      <li><img src=\"/img/logo_zoes.png\" alt=\"Zoe's Kitchen\"></li>\n      <li><img src=\"/img/logo_madammams.png\" alt=\"Madam Mam's\"></li>\n    </ul>\n  </div>\n</section>\n\n<div class=\"modal fade\" id=\"contact-us-modal\">\n  <div class=\"modal-dialog\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h3 class=\"modal-title\">Contact Us</h3>\n      </div>\n      <form action=\"#\" class=\"form form-vertical contact-us-form\">\n        <div class=\"modal-body\">\n          <div class=\"form-group\">\n            <label for=\"name-input\">Name</label>\n            <input type=\"text\" name=\"name\">\n          </div>\n          <div class=\"form-group\">\n            <label for=\"name-input\">Email</label>\n            <input type=\"email\" name=\"email\">\n          </div>\n          <div class=\"form-group\">\n            <label for=\"message-input\">Message</label>\n            <textarea rows=\"6\" type=\"text\" name=\"message\"></textarea>\n          </div>\n        </div>\n        <div class=\"modal-footer\">\n          <button type=\"button\" class=\"btn btn-default\" data-dismiss=\"modal\">Close</button>\n          <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</div>\n\n<footer class=\"footer\">\n  <ul class=\"nav footer-nav\">\n    <li><a href=\"#\" data-toggle=\"modal\" data-target=\"#contact-us-modal\">Contact Us</a></li>\n    <li><a href=\"https://www.goodybag.com/legal\">Terms of service</a></li>\n    <li><a href=\"https://www.goodybag.com/privacy\">Privacy policy</a></li>\n  </ul>\n</footer>\n\n    <script src=\"/dist/app.js\"></script>\n    \n  \n",
            "exportPath": "/pinf.genesis.consulting/GoodyBag/goodybag-core/07-lunchroom-landing/.sm.hoisted/lunchroom-landing~0/components/Landing/page.htm"
