        "checkout-form": {
            "tag": "DIV",
            "attributes": {
                "class": "container checkout-info-container"
            },
            "innerHTML": "\n  <fieldset class=\"checkout-info form-vertical\">\n    <div class=\"user-info\">\n      <h3 class=\"section-title\">Your Info</h3>\n      <div class=\"form-group\">\n        <label for=\"\">Name</label>\n        <input type=\"text\" class=\"form-control\" component:elm=\"info[name]\">\n      </div>\n      <div class=\"form-group\">\n        <label for=\"\">Email</label>\n        <input type=\"email\" class=\"form-control\" component:elm=\"info[email]\">\n      </div>\n      <div class=\"form-group\">\n        <label for=\"\">Phone Number</label>\n        <input type=\"tel\" class=\"form-control\" component:elm=\"info[phone]\">\n      </div>\n      <p class=\"checkout-info-note\">We'll email/text you when your meal has arrived!</p>\n    </div>\n    <div class=\"payment-info\">\n      <h3 class=\"section-title\">Payment Info</h3>\n        <div class=\"form-group form-group-member-name\">\n          <label for=\"\">Name on card</label>\n          <input type=\"text\" class=\"form-control\" name=\"card_member_name\" component:elm=\"card[name]\">\n        </div>\n        <div class=\"inline-form-group-list\">\n          <div class=\"form-group form-group-card-number\">\n            <label for=\"\">Card number</label>\n            <input type=\"text\" class=\"form-control\" name=\"card_number\" component:elm=\"card[number]\">\n          </div>\n          <div class=\"form-group form-group-card-cvv\">\n            <label for=\"\">CVV</label>\n            <input type=\"text\" class=\"form-control\" name=\"card_cvv\" component:elm=\"card[cvc]\">\n          </div>\n          <div class=\"form-group form-group-expiration\">\n            <label for=\"\">Expiration date</label>\n            <input type=\"text\" class=\"form-control\" placeholder=\"MM\" name=\"card_expiration_month\" component:elm=\"card[expire-month]\">\n            /\n            <input type=\"text\" class=\"form-control\" placeholder=\"YY\" name=\"card_expiration_year\" component:elm=\"card[expire-year]\">\n          </div>\n        </div>\n    </div>\n  </fieldset>\n",
            "exportPaths": {
                "htm": "/pinf.genesis.consulting/GoodyBag/goodybag-core/07-lunchroom-landing/.sm.hoisted/lunchroom-landing~0/components/AppCheckout/checkout-form.htm",
