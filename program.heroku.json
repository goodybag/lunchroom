{
	"env": {
        "PORT": "$PORT"
    },
	"boot": {
        "config": "lunchroom/0",
        "package": "./package.json"
    },
    "config": {
        "lunchroom/0": {
            "port": "{{env.PORT}}",
            "db": {
                "postgresql": {
                    "user": "lunchroom_dev",
                    "password": "1E07DEEC-DE21-4E84-B47F-104C5070259A",
                    "database": "lunchroom_dev",
                    "host": "192.241.203.208",
                    "port": 8089,
                    "ssl": true
                }
            },
            "firenode": {
				"config": {
	                "skinUrl": "//lunchroom-stage.goodybag.com:8083/lunchroom/assets/semantic-ui-css/semantic.css",
	                "assetsUrl": "/assets.js",
	                "bundleUrl": "/bundle.js",
	                "clientContext": {
	                    "dev": false,
	                    "type": "lunchroom",
	                    "overrideSkinUri": "/dev.skin.style.css"
	                },
	                "db": {
	                    "dbname": "lunchroom stage DB"
	                }
                },
                "routes": {
                    "/": {
                        "allow": true
                    },
                    "/$": {
                        "config": {
                            "router": {
                                "impl": "07-lunchroom/consumer-group-router/0"
                            },
                            "internalUri": "/harness.htm",
                            "clientContext": {
                                "sessionCookieName": "gblsid",
                                "selectedView": "Home",
                                "lockedView": "Home"
                            }
                        }
                    },
                    "/admin-A3C9D2B6-201C-422E-BF63-D536C480332F": {
                    	"lastMatchingRoute": true,
                        "config": {
                            "internalUri": "/harness.htm",
                            "clientContext": {
                                "selectedView": "",
                                "type": "",
                                "overrideSkinUri": ""
                            }
                        }
                    },
                    "/components/GBL_DEV_Views/mockups/": {},
                    "/api/v1/": {
                        "lastMatchingRoute": true,
                        "config": {
                            "clientContext": {
                                "sessionCookieName": "gblsid"
                            }
                        }
                    },
                    "^/a/cs/(.+)$": {
                        "lastMatchingRoute": true,
                        "config": {
                            "router": {
                                "impl": "07-lunchroom/consumer-group-subscription-router/0",
                                "action": "confirm"
                            }
                        }
                    },
                    "^/a/us/(.+)$": {
                        "lastMatchingRoute": true,
                        "config": {
                            "router": {
                                "impl": "07-lunchroom/consumer-group-subscription-router/0",
                                "action": "unsubscribe"
                            }
                        }
                    },
                    "^/(.+)$": {
                        "config": {
                            "sessionToken": "<REPLACED-BY-ROUTER>",
                            "router": {
                                "impl": "07-lunchroom/consumer-group-router/0"
                            },
                            "skinUrl": "/dist/landing-page.css",
                            "internalUri": "/landing.htm",
                            "clientContext": {
                                "overrideSkinUri": "/landing.skin.style.css",
                                "sessionCookieName": "gblsid",
                                "selectedView": "Landing",
                                "lockedView": "Landing,ContactUs,TermsOfService,PrivacyPolicy"
                            }
                        }
                    },
                    "^/(.+)/.*$": {
                        "config": {
                            "clientContext": {
                                "selectedView": "Menu_Web",
                                "lockedView": ""
                            }
                        }
                    }
                },
                "hosts": {
                    "goodybag-lunchroom-staging.herokuapp.com": {
